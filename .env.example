# Copy this to .env and fill in secrets. Do NOT commit .env
# Postgres connection string for Neon (preferred):
NEON_DATABASE_URL=postgresql://<role>:<password>@<host>/<db>?sslmode=require

# Alternatively, Neon REST endpoint (not used by default if NEON_DATABASE_URL is present)
NEON_REST_URL=https://.../rest/v1
NEON_API_KEY=

# JWKS URL for admin JWT verification
NEON_JWKS_URL=https://api.stack-auth.com/api/v1/projects/255e0e4d-4784-47ce-8a66-8889b6e78fbd/.well-known/jwks.json
ADMIN_ISS=
ADMIN_AUD=
ADMIN_ROLE_CLAIM=role
ADMIN_ROLE_VALUE=admin

# Server port
PORT=4000

# Static admin token fallback (if you don't configure NEON_JWKS_URL)
# Set a long random value here and keep it secret. Example:
# ADMIN_TOKEN=supersecrettoken123
